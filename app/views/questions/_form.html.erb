<%= form_with(model: question, local: true) do |form| %>
  

  <div class="field">
    <%= form.label :question_name %>
    <%= form.text_field :question_name, id: :que %>
  </div>

  <div class="field">
    <%= form.label :que_priority %>
    <%= form.text_field :que_priority, id: :que %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
